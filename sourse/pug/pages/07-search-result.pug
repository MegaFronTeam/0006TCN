	- let  bg= '07-basket-1.jpg'

block title
	- let  title= "Результаты поиска"
	title= title
block pagecontent
	//- +mob-mnu()
	
	
	+header()
		+topLine 
		+top-nav()
	.container
		.row
			.col-lg-9
				+page-head(title, 'Главная', title)
				+sSearchResult()
			.col-lg-3
				+needHelp
	
	+footer()
      



